// Declarative Pipeline
pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                sh '''#!/bin/bash
                    ls
                    echo $SHELL
                    docker build --tag=local/ubuntu:casc .
                '''
             }
        }
        stage('Image List') {
            steps {
                sh '''#!/bin/bash
                    docker images
                '''
            }
        }
    }
}
